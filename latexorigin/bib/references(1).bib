	title = {Rayleigh–{Taylor} instability of cylindrical water droplet induced by laser-produced cavitation bubble},
	volume = {919},
	issn = {1469-7645},
	url = {https://doi.org/10.1017/jfm.2021.401},
	doi = {10.1017/jfm.2021.401},
	abstract = {This study gives insights into the interfacial instability of water droplets through a combination of laboratory experiments, numerical simulations and analytical modelling. An experiment is conducted in a narrow gap between two plates to model the two-dimensional cylindrical geometry, and a pulsed laser beam is focused inside a water droplet to generate a cavitation bubble. Three distinct characteristics of droplet deformation can be observed: (i) splashing; (ii) ventilating; and (iii) a stable state. In addition, an analytical model considering the Rayleigh–Taylor instability and bubble oscillation is developed based on the assumption that fluid is inviscid and incompressible. The analytical model is solved to obtain a phase diagram describing three distinct phenomena. Two dimensionless parameters, \${\textbackslash}hat \{{\textbackslash}eta \}\_\{ins\}\$ and \${\textbackslash}hat \{{\textbackslash}eta \}\_\{sta\}\$, are used to determine the boundaries between different regimes. The parameter \${\textbackslash}hat \{{\textbackslash}eta \}\_\{ins\}\$ is defined by the ratio of the perturbation amplitude to the difference between the droplet and bubble radii, whereas the other parameter \${\textbackslash}hat \{{\textbackslash}eta \}\_\{sta\}\$ is defined by the ratio of the perturbation amplitude to the initial droplet radius. Interfacial instability is induced when the perturbed droplet surface penetrates the bubble boundary, namely, \${\textbackslash}rvert {\textbackslash}hat \{{\textbackslash}eta \}\_\{ins\} {\textbackslash}rvert {\textbackslash}geq 1\$. Splashing and ventilating phenomena occur for \${\textbackslash}hat \{{\textbackslash}eta \}\_\{ins\}{\textbackslash}geq 1\$ and \${\textbackslash}hat \{{\textbackslash}eta \}\_\{ins\}{\textbackslash}leq -1\$, respectively. A stable state occurs when droplet fragmentation does not appear despite small-amplitude perturbations for \${\textbackslash}hat \{{\textbackslash}eta \}\_\{sta\}{\textbackslash}leq 0.1\$. There is a transition zone between the ventilating and stable state, which is bounded by \${\textbackslash}hat \{{\textbackslash}eta \}\_\{ins\} = -1\$ and \${\textbackslash}hat \{{\textbackslash}eta \}\_\{sta\} = 0.1\$. Finally, the phase diagram is verified by the experimental and numerical results.},
	journal = {Journal of Fluid Mechanics},
	author = {Wang, Jingzhu and Li, Hongchen and Guo, Wenlu and Wang, Zhan and Du, Tezhuan and Wang, Yiwei and Abe, Akihisa and Huang, Chenguang},
	year = {2021},
	keywords = {/unread, ⛔ No INSPIRE recid found},
	pages = {A42--undefined},
}

