	author = {Vogel, A. and Nahen, K. and Theisen, D. and Noack, J.},
	month = dec,
	year = {1996},
	note = {Conference Name: IEEE Journal of Selected Topics in Quantum Electronics},
	keywords = {/unread, Biomedical optical imaging, Electric breakdown, Free electron lasers, Ionization, Laser modes, Laser theory, Nd:YAG lasers, Optical pulses, Plasma measurements, Plasma waves, Plasmas, Pulse measurements, Reflection, Scattering, Water, avalanche breakdown, biological effects of laser ra, biological effects of laser radiation, eye, high-spe},
