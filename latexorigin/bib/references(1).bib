}

@article{83ff50c4c8b945d52458334f5d25c23b235b565e,
	title = {Non-gated laser-induced breakdown spectroscopy in bulk water by position-selective detection},
	volume = {107},
	url = {https://www.semanticscholar.org/paper/83ff50c4c8b945d52458334f5d25c23b235b565e},
	doi = {10.1063/1.4931128},
	abstract = {Temporal and spatial evolutions of the laser-induced plasma in bulk water are investigated using fast imaging and emission spectroscopic techniques. By tightly focusing a single-pulse nanosecond Nd: YAG laser beam into the bulk water, we generate a strongly expanded plasma with high reproducibility. Such a strong expanding plasma enables us to obtain well-resolved spectral lines by means of position-selective detection; hence, the time-gated detector becomes abdicable. The present results suggest not only a possible non-gated approach for underwater laser-induced breakdown spectroscopy but also give an insight into the plasma generation and expansion in bulk water.},
	journal = {Applied Physics Letters},
	author = {Tian, Ye and Xue, Boyang and Song, Jiaojian and Lu, Yuan and Zheng, R.},
	year = {2015},
