@article{antkowiak_short-term_2007,
	title = {Short-term dynamics of a density interface following an impact},
	volume = {577},
	issn = {0022-1120, 1469-7645},
	url = {https://www.cambridge.org/core/product/identifier/S0022112007005058/type/journal_article},
	doi = {10.1017/S0022112007005058},
	abstract = {A tube filled with a perfectly wetting liquid falls axially under its own weight. In its gravity-free reference frame, the liquid interface is deformed by surface tension into a hemispherical shape. On impact of the tube on a rigid floor, the interface curvature reverses violently, forming a concentrated jet. If the contact angle at the tube wall is such that the interface is flat, the liquid rebounds as a whole with the tube, with no deformation. We analyse this phenomenon using an impulse pressure description, providing an exact description of the initial liquid velocity field at the impact, supported by high-speed image velocimetry measurements. This initial dynamics is insensitive to liquid surface tension and viscosity.},
