}

@article{zhang_experimental_2015,
	title = {Experimental study on bubble dynamics subject to buoyancy},
	volume = {776},
	issn = {0022-1120, 1469-7645},
	url = {https://www.cambridge.org/core/product/identifier/S0022112015003237/type/journal_article},
