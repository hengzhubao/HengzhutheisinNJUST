}

@article{tian_laser-induced_2020,
	title = {Laser-induced plasma in water at high pressures up to 40 {MPa}: {A} time-resolved study},
	volume = {28},
	copyright = {© 2020 Optical Society of America},
	issn = {1094-4087},
	shorttitle = {Laser-induced plasma in water at high pressures up to 40 {MPa}},
	url = {https://opg.optica.org/oe/abstract.cfm?uri=oe-28-12-18122},
	doi = {10.1364/OE.391420},
	abstract = {The knowledge on the laser-induced plasma emission in water at high pressures is essential for the application of laser-induced breakdown spectroscopy (LIBS) in the deep-sea. In this work, we investigate the spectral features of ionic, atomic and molecular emissions for the plasma in water at different pressures from 1 to 40 MPa. By comparing between the time-resolved spectra and shadowgraph images, we demonstrate that the dynamics of the cavitation bubble at high pressures plays a key role on the characterization of plasma emission. The initial plasma emission depends weakly on the external pressure. As time evolves, the cavitation bubble is more compressed by the higher external pressure, leading to a positive confinement effect to maintain the plasma emission. However, at very high pressures, the bubble collapses extremely fast and even earlier than the cooling of the plasma. The plasma will gain energy from the bubble collapse phase, but quench immediately after the collapse, leading to a sharp reduction in the plasma persistence. These effects caused by bubble dynamics explain well the observed spectral features and are further proved by the temporal evolutions of the plasma temperature and electron density. This work gives not only some insights into the laser-induced plasma and bubble dynamics in high pressure liquids but also better understanding for the application of underwater LIBS in the deep-sea.},
	language = {EN},
	number = {12},
	urldate = {2023-02-24},
	journal = {Optics Express},
	author = {Tian, Ye and Li, Ying and Wang, Lintao and Huang, Fuzhen and Lu, Yuan and Guo, Jinjia and Zheng, Ronger},
	month = jun,
	year = {2020},
	note = {Publisher: Optica Publishing Group},
	keywords = {Chemical sensors, Laser energy, Laser induced breakdown, Laser irradiation, Laser medical applications, Long pulse operation, ⛔ No INSPIRE recid found},
	pages = {18122--18130},
}

