	author = {Peshkovsky, Sergei L. and Peshkovsky, Alexey S.},
	month = mar,
	year = {2007},
	keywords = {Acoustic energy transference, Acoustic horns, Acoustic impedance matching, Cavitation, Electromechanical transducers, Industrial processes in liquids, Industrial ultrasonic reactors, Sonochemistry, Ultrasonic power transference, Ultrasonic rod horns},
	pages = {314--322},
}

@article{apitz_material_2005,
	title = {Material ejection in nanosecond {Er}:{YAG} laser ablation of water, liver, and skin},
