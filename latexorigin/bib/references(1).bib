@inproceedings{stolarski_integrated_1995,
	address = {San Jose, CA, United States},
	title = {Integrated light spectroscopy of laser-induced breakdown in aqueous media},
	url = {http://proceedings.spiedigitallibrary.org/proceeding.aspx?doi=10.1117/12.209873},
	doi = {10.1117/12.209873},
	abstract = {In the scientific literature there is little information that describes the fundamental physical processes of laser induced breakdown (LIB) in transparent liquids. Our goal is to characterize these fundamental properties, which are critical to the understanding of retinal and other ophthalmic damage produced by ultrashort laser pulses. Laser pulses of 5.0 nanoseconds (ns) at less than 5.0 milli-Joules (mJ) per pulse and pulses of 80 picoseconds (ps) at 0.5 to 1 .5 mJ per pulse from a Nd:YAG regenerative amplifier were used to produce LIB in a variety of aqueous media. These include physiological saline solution, triple-distilled water, and tap water. The resulting luminescent plasmas were analyzed using integrated light spectroscopy from a Chromex 0.25 meter (m) spectrograph. Plasmas were recorded in the wavelength region from 300 to 900 nm. Each spectrum obtained was analyzed using a Planck distribution for blackbody emission. The surface temperatures of the plasmas for the two pulse durations were computed to be in the 5000 K to 10,000 K range, depending on the pulse duration and energy. Also, the spectrographs from the saline solution included distinct spectral lines of emission over the broad band spectra, such as the 589 nm atomic emission line of sodium. We will discuss the time-integrated spectroscopy of LIB in the various solutions, and how LIB might mediate retinal damage induced by ultrashort laser pulses.},
	language = {en},
	urldate = {2022-02-28},
	author = {Stolarski, David J. and Hardman, Jennifer M. and Bramlette, Craig M. and Noojin, Gary D. and Thomas, Robert J. and Rockwell, Benjamin A. and Roach, William P.},
	editor = {Jacques, Steven L.},
	month = may,
	year = {1995},
	keywords = {/unread, Laser induced damage, Plasmas, Spectroscopy, Ultrafas, laser induced breakdown, nonlinear, plasma spectroscopy in liquids, plasma temperature, properties of liquids, ultrashort laser pulse},
	pages = {100},
}

